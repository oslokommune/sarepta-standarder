<schema xmlns:m244="http://www.kith.no/xmlstds/eresept/m244/2013-10-15" xmlns:xsd="http://www.w3.org/2001/XMLSchema.xsd" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:kith="http://www.kith.no/xmlstds" targetNamespace="http://www.kith.no/xmlstds/eresept/m244/2013-10-15" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<import namespace="http://www.kith.no/xmlstds" schemaLocation="../felles/kith.xsd"/>
	<element name="SamtykkeEndring">
		<annotation>
			<documentation>Informasjon fra Reseptformidler til lege om flyttede pasientsamtykker. </documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="HerIdGammel" type="string" minOccurs="0">
					<annotation>
						<documentation>HER-id der samtykker flyttes fra. </documentation>
					</annotation>
				</element>
				<element name="HerIdNy" type="string">
					<annotation>
						<documentation>HER-id der samtykker er flyttet til. </documentation>
					</annotation>
				</element>
				<element name="HprNr" type="string">
					<annotation>
						<documentation>Den flyttende legens HPR-nummer. </documentation>
					</annotation>
				</element>
				<choice>
					<element name="FnrPasient" type="string" maxOccurs="unbounded">
						<annotation>
							<documentation>Pasienters f√∏dselsnumre.</documentation>
						</annotation>
					</element>
					<element name="IngenEndring" type="boolean"/>
				</choice>
			</sequence>
		</complexType>
	</element>
</schema>
