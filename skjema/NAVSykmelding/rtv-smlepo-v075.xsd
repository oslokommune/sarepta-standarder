<!--		SykmeldGjelder 1..4 -> 1..3							-->
<!--		New annotations on some elements				-->
<schema targetNamespace="http://www.kith.no/xmlstds/sykmelding/2004-05-03" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:rtv="http://www.kith.no/xmlstds/sykmelding/2004-05-03" xmlns:xsd="schemaLocation for xmlns:xsd http://www.w3.org/2001/XMLSchema.xsd" xmlns="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="http://www.w3.org/TR/xmldsig-core/xmldsig-core-schema.xsd"/>
	<!--

SYKMELDING

-->
	<element name="Sykmelding">
		<annotation>
			<documentation>Medisinsk vurdering av arbeidsmulighet ved sykdom</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="SykmeldGjelder" type="rtv:typeSykmelding" maxOccurs="3">
					<annotation>
						<documentation>[0.1-0.4] For å angi om det er første medisinske vurdering, påfølgende vurdering, endringer og/eller informasjon i del II</documentation>
					</annotation>
				</element>
				<element name="Pasientopplysninger">
					<annotation>
						<documentation>[1] Opplysninger om personen og arbeidsforholdet</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="Navn" type="rtv:typeNavn">
								<annotation>
									<documentation>[1.1] Pasientens navn</documentation>
								</annotation>
							</element>
							<element name="Fodselsnummer" type="string">
								<annotation>
									<documentation>[1.2] Fødselsnummer (11 siffer)</documentation>
								</annotation>
							</element>
							<element ref="rtv:Arbeidsforhold" minOccurs="0"/>
							<element name="Leder" minOccurs="0">
								<annotation>
									<documentation>[1.3.1] Navn på nærmeste leder</documentation>
								</annotation>
							</element>
							<element name="FlereArbeidsforhold" type="rtv:typeJaNei" minOccurs="0">
								<annotation>
									<documentation>[1.4]</documentation>
								</annotation>
							</element>
							<element name="Identifikasjon" minOccurs="0">
								<annotation>
									<documentation>[1.5] For å angi om pasienten er kjent eller har vist dokumentasjon</documentation>
								</annotation>
								<complexType>
									<choice>
										<element name="Kjent" type="boolean">
											<annotation>
												<documentation>[1.5] Angir om personen er kjent.</documentation>
											</annotation>
										</element>
										<element name="Legitimasjon" type="boolean">
											<annotation>
												<documentation>[1.5.1] Angir om legitimasjon er vist.</documentation>
											</annotation>
										</element>
									</choice>
								</complexType>
							</element>
							<element name="Trygdekontor" type="string" minOccurs="0">
								<annotation>
									<documentation>[1.6]</documentation>
								</annotation>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="Arbeidsmulighet" minOccurs="0">
					<annotation>
						<documentation>[2] Arbeidsmulighet / forenklet funksjonsvurdering</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="Arbeidsoppgaver" type="rtv:typeTekst" minOccurs="0">
								<annotation>
									<documentation>[2.1.1]</documentation>
								</annotation>
							</element>
							<element name="ForsteFraversdag" type="date" minOccurs="0">
								<annotation>
									<documentation>[2.2] Pasientens opplysning om første fraværsdag.</documentation>
								</annotation>
							</element>
							<element name="ForsteKonsultasjon" type="date" minOccurs="0">
								<annotation>
									<documentation>[2.3] Legens opplysninger om når første konsultasjon har vært utført.</documentation>
								</annotation>
							</element>
							<element name="DatoTelefonKonsultasjon" type="date" minOccurs="0">
								<annotation>
									<documentation>[2.4] Dato for når telefonkonsultasjon er utført.</documentation>
								</annotation>
							</element>
							<element name="Behandlingsdager" type="rtv:typeTekst" minOccurs="0">
								<annotation>
									<documentation>[2.5] Enkeltstående behandlingsdager</documentation>
								</annotation>
							</element>
							<element name="Reisetilskudd" type="rtv:typeVarighetDato" minOccurs="0">
								<annotation>
									<documentation>[2.6] Kan være på arbeid forutsatt reisetilskudd f.o.m.-t.o.m.</documentation>
								</annotation>
							</element>
							<element name="UtforeEgetArbeid" minOccurs="0" maxOccurs="unbounded">
								<annotation>
									<documentation>[2.7] Kan delvis utføre eget arbeid f.o.m.-t.o.m.</documentation>
								</annotation>
								<complexType>
									<complexContent>
										<extension base="rtv:typeVarighetDato">
											<sequence>
												<element name="Grad" type="integer"/>
											</sequence>
										</extension>
									</complexContent>
								</complexType>
							</element>
							<element name="AktivSykmelding" type="rtv:typeVarighetDato" minOccurs="0">
								<annotation>
									<documentation>[2.8] Kan være i aktiv sykmelding f.o.m.-t.o.m.</documentation>
								</annotation>
							</element>
							<element name="Hensyn" minOccurs="0">
								<annotation>
									<documentation>[2.9] Hvilke hensyn bør eventuelt tas for at personen kan være i delvis arbeid/arbeidsrelatert aktivitet?</documentation>
								</annotation>
							</element>
							<element name="Eventuelt" type="rtv:typeTekst" minOccurs="0">
								<annotation>
									<documentation>[2.10] Eventuelle innspill til tilrettelegging på arbeidsplassen</documentation>
								</annotation>
							</element>
							<element name="IkkeJobb" type="rtv:typeVarighetDato" minOccurs="0">
								<annotation>
									<documentation>[2.11] Kan ikke være i arbeidsrelatert aktivitet f.o.m. - t.o.m.</documentation>
								</annotation>
							</element>
							<element name="MedisinskeBehArsaker" type="rtv:typeJaNei" minOccurs="0">
								<annotation>
									<documentation>[2.11.1] Er det tungtveiende medisinske eller behandlingsrettede årsaker til at personen ikke kan være i arbeidsrelatert aktivitet?</documentation>
								</annotation>
							</element>
							<element name="Tilpasning" minOccurs="0">
								<annotation>
									<documentation>[2.12] Antas personen på sikt å:</documentation>
								</annotation>
								<complexType>
									<sequence>
										<element name="Gjenoppta" type="rtv:typeTilpasning" minOccurs="0">
											<annotation>
												<documentation>[2.12 a] kunne gjenoppta nåværende arbeid</documentation>
											</annotation>
										</element>
										<element name="Tilrettelegging" type="rtv:typeTilpasning" minOccurs="0">
											<annotation>
												<documentation>[2.12 b] ha behov for tilrettelegging i arbeid</documentation>
											</annotation>
										</element>
										<element name="AnnetArbeid" type="rtv:typeTilpasning" minOccurs="0">
											<annotation>
												<documentation>[2.12 c] ha behov for annet arbeid</documentation>
											</annotation>
										</element>
									</sequence>
								</complexType>
							</element>
							<element name="ArbeidsrelatertAktivitet" type="rtv:typeJaNei" minOccurs="0">
								<annotation>
									<documentation>[2.13] Er personen idag i arbeidsrelatert aktivitet? (fylles ut ved alle påfølgende vurderinger). Hvis nei, vurder pkt. 4</documentation>
								</annotation>
							</element>
							<element name="Friskmeldt" type="string" minOccurs="0">
								<annotation>
									<documentation>[2.14] Forventet friskmeldt (uke)</documentation>
								</annotation>
							</element>
							<element name="Friskmeldingsdato" type="date" minOccurs="0">
								<annotation>
									<documentation>[2.15]</documentation>
								</annotation>
							</element>
							<element name="FriskmeldingArbeidsformidling" type="rtv:typeTekst" minOccurs="0">
								<annotation>
									<documentation>[2.16] Friskmelding til arbeidsformidling (gjelder kun for person uten arbeidsgiver)</documentation>
								</annotation>
							</element>
						</sequence>
					</complexType>
					<!-- 	Felt 2.1 Yrke er definert som
							/Sykmelding/Pasientopplysninger/Arbeidsforhold/@yrkeskode
							/Sykmelding/Pasientopplysninger/Arbeidsforhold/Yrkesbetegnelse
					 -->
				</element>
				<element name="DiagnoseMv" minOccurs="0">
					<annotation>
						<documentation>[3] For å angi om fravær fra arbeid har andre årsaker enn egen sykdom, om sykdommen er svangerskapsrelatert, og evt. angivelse av skadedato for yrkessykdom/yrkesskade.</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="DiagnoseKodesystem" minOccurs="0">
								<annotation>
									<documentation>For å angi benyttet diagnosekodesystem.</documentation>
								</annotation>
								<complexType>
									<sequence>
										<element name="Kodesystem" type="rtv:typeKodesystem">
											<annotation>
												<documentation>[3.2] Lovlige verdier er 3 (ICD-10) eller 5 (ICPC-2)</documentation>
											</annotation>
										</element>
										<element name="Enkeltdiagnose" maxOccurs="unbounded">
											<annotation>
												<documentation>[3.1.2] For å angi kodeverdi, tekstlig beskrivelse av kodeverdi og sorteringsrekkefølge for enkeltdiagnoser (en pasient kan ha flere diagnoser). En hoveddiagnose må alltid oppgis.</documentation>
											</annotation>
											<complexType>
												<sequence>
													<element name="Diagnose" type="string" minOccurs="0">
														<annotation>
															<documentation>[3.1]</documentation>
														</annotation>
													</element>
													<element name="Kodeverdi" type="string">
														<annotation>
															<documentation>[3.1.2]</documentation>
														</annotation>
													</element>
													<element name="Sortering" type="nonNegativeInteger" default="0"/>
												</sequence>
											</complexType>
										</element>
									</sequence>
								</complexType>
							</element>
							<element name="Svangerskap" type="boolean" minOccurs="0">
								<annotation>
									<documentation>[3.1.1]</documentation>
								</annotation>
							</element>
							<element name="AnnenFraversgrunn" type="rtv:typeTekst" minOccurs="0"/>
							<element name="Yrkessykdom" type="boolean" minOccurs="0">
								<annotation>
									<documentation>[3.4.1]</documentation>
								</annotation>
							</element>
							<element name="Yrkesskade" type="boolean" minOccurs="0">
								<annotation>
									<documentation>[3.4.2]</documentation>
								</annotation>
							</element>
							<element name="SkadeDato" type="date" minOccurs="0">
								<annotation>
									<documentation>[3.4.2.1]</documentation>
								</annotation>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="MedisinskeArsaker" minOccurs="0">
					<annotation>
						<documentation>[4] Del II. Medisinske årsaker til at arbeidsrelatert aktivitet ikke er igangsatt</documentation>
					</annotation>
					<complexType>
						<sequence>
							<element name="SykdommenSelv" type="rtv:typeJaNei" minOccurs="0">
								<annotation>
									<documentation>[4.1] Sykdommen i seg selv</documentation>
								</annotation>
							</element>
							<element name="Behandlingsopplegg" type="rtv:typeJaNei" minOccurs="0">
								<annotation>
									<documentation>[4.2] Behandlingsopplegg</documentation>
								</annotation>
							</element>
							<element name="AndreArsaker" type="rtv:typeJaNei" minOccurs="0">
								<annotation>
									<documentation>[4.3] Andre årsaker</documentation>
								</annotation>
							</element>
							<element name="Begrunnelse" type="rtv:typeTekst" minOccurs="0">
								<annotation>
									<documentation>[4.4] Hvis ja i punkt 4.1, 4.2 eller 4.3, begrunn</documentation>
								</annotation>
							</element>
							<element name="StatusPresens" type="rtv:typeTekst" minOccurs="0">
								<annotation>
									<documentation>[4.5] Symptomer og status presens</documentation>
								</annotation>
							</element>
							<element name="ForslagTiltak" type="rtv:typeTekst" minOccurs="0">
								<annotation>
									<documentation>[4.6] Forslag til mulige tiltak (til arbeidstaker / arbeidsgiver)</documentation>
								</annotation>
							</element>
							<element name="KontaktBht" type="boolean" minOccurs="0">
								<annotation>
									<documentation>[4.7] Foreslår kontakt med BHT.</documentation>
								</annotation>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="Merknad" type="rtv:typeTekst" minOccurs="0">
					<annotation>
						<documentation>[5] Merknader til trygdekontoret</documentation>
					</annotation>
				</element>
				<element ref="rtv:Legeunderskrift"/>
				<element ref="ds:Signature" minOccurs="0"/>
			</sequence>
			<attribute name="formOfLanguage" use="required">
				<simpleType>
					<restriction base="string">
						<enumeration value="1">
							<annotation>
								<documentation>Bokmål</documentation>
							</annotation>
						</enumeration>
						<enumeration value="2">
							<annotation>
								<documentation source="Nynorsk"/>
							</annotation>
						</enumeration>
					</restriction>
				</simpleType>
			</attribute>
			<attribute name="sendersId" type="string" use="optional"/>
			<attribute name="receiversId" type="string" use="optional"/>
			<attribute name="schemaVersion" type="string" use="required" fixed="IA 08-07.04 Fastsatt 6.88 Endret 06.2004 Elektronisk XML 3.0"/>
			<attribute name="messageId" type="string" use="required"/>
			<attribute name="genDate" type="dateTime" use="required"/>
		</complexType>
	</element>
	<simpleType name="typeSykmelding">
		<annotation>
			<documentation>Kodeverdier 1-4</documentation>
		</annotation>
		<restriction base="nonNegativeInteger">
			<enumeration value="1">
				<annotation>
					<documentation>[0.1] Første medisinske vurdering</documentation>
				</annotation>
			</enumeration>
			<enumeration value="2">
				<annotation>
					<documentation>[0.2] Påfølgende vurdering</documentation>
				</annotation>
			</enumeration>
			<enumeration value="3">
				<annotation>
					<documentation>[0.3] Endringer</documentation>
				</annotation>
			</enumeration>
			<enumeration value="4">
				<annotation>
					<documentation>[0.4] Informasjon i Del II</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>
	<simpleType name="typeJaNeiVetikke">
		<annotation>
			<documentation>Kodeverdier 1-3</documentation>
		</annotation>
		<restriction base="nonNegativeInteger">
			<enumeration value="1">
				<annotation>
					<documentation>Ja</documentation>
				</annotation>
			</enumeration>
			<enumeration value="2">
				<annotation>
					<documentation>Nei</documentation>
				</annotation>
			</enumeration>
			<enumeration value="3">
				<annotation>
					<documentation>Vet ikke / Usikkert</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>
	<complexType name="typeVarighetDato">
		<annotation>
			<documentation>Varighet mellom to datoer.</documentation>
		</annotation>
		<sequence>
			<element name="FraDato" type="date"/>
			<element name="TilDato" type="date"/>
		</sequence>
	</complexType>
	<complexType name="typeTilpasning">
		<sequence>
			<element name="Svar" type="rtv:typeJaNeiVetikke"/>
			<element name="Uke" type="string" minOccurs="0">
				<annotation>
					<documentation>Hvis Ja, anslå uke nr</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<!--
	
LEGEERKLÆRING
	
-->
	<element name="Legeerklaring">
		<annotation>
			<documentation>Spesialisering av den abstrakte klassen Melding, til  klassen Legeerklæring ved arbeidsuførhet.</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element ref="rtv:LegeerklaringGjelder" maxOccurs="4"/>
				<element ref="rtv:Pasientopplysninger"/>
				<element ref="rtv:PlanUtredBehandle" minOccurs="0"/>
				<element ref="rtv:DiagnoseArbeidsuforhet"/>
				<element ref="rtv:ForslagTiltak"/>
				<element ref="rtv:VurderingFunksjonsevne"/>
				<element ref="rtv:Prognose"/>
				<element ref="rtv:ArsakssammenhengLegeerklaring" minOccurs="0"/>
				<element ref="rtv:ForbeholdLegeerklaring"/>
				<element ref="rtv:AndreOpplysninger" minOccurs="0"/>
				<element ref="rtv:Legeunderskrift"/>
				<element ref="rtv:Kontakt" minOccurs="0" maxOccurs="unbounded"/>
				<element ref="ds:Signature" minOccurs="0"/>
			</sequence>
			<attribute name="formOfLanguage" type="rtv:typeFormOfLanguage" use="required"/>
			<attribute name="sendersId" type="string" use="optional"/>
			<attribute name="receiversId" type="string" use="optional"/>
			<attribute name="schemaVersion" type="string" use="required" fixed="IA 08-07.08 Fastsatt 10.99 Elektronisk 1.1"/>
			<attribute name="messageId" type="string" use="required"/>
			<attribute name="genDate" type="dateTime" use="required"/>
		</complexType>
	</element>
	<element name="VurderingFunksjonsevne">
		<annotation>
			<documentation>For å beskrive hvordan pasientens generelle funksjonsevne er nedsatt.</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element ref="rtv:Arbeidssituasjon" maxOccurs="unbounded"/>
				<element ref="rtv:VurderingArbeidsevne" minOccurs="0"/>
				<element name="Funksjonsevne" type="rtv:typeTekst" minOccurs="0"/>
				<element name="KravArbeid" type="rtv:typeTekst" minOccurs="0"/>
			</sequence>
		</complexType>
	</element>
	<element name="ArsakssammenhengLegeerklaring" type="rtv:typeTekst">
		<annotation>
			<documentation>For å angi hvor stor betydning funksjonsnedsettelsen har for at arbeidsevnen er nedsatt.</documentation>
		</annotation>
	</element>
	<element name="ForbeholdLegeerklaring">
		<annotation>
			<documentation>For å angi om det er noe i legeerklæringen som pasienten ikke bør få vite av medisinske grunner.</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="BorTilbakeholdes" type="rtv:typeTekst" minOccurs="0"/>
			</sequence>
			<attribute name="tilbakeholdInnhold" type="rtv:typeJaNei" use="optional"/>
		</complexType>
	</element>
	<element name="AndreOpplysninger">
		<annotation>
			<documentation>For å angi andre opplysninger.</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="Opplysning" type="rtv:typeTekst"/>
			</sequence>
			<attribute name="onskesKopi" type="rtv:typeJaNei" use="optional"/>
		</complexType>
	</element>
	<element name="Kontakt">
		<annotation>
			<documentation>For å angi hvem legen ønsker at trygdekontoret skal ta kontakt med.</documentation>
		</annotation>
		<complexType>
			<attribute name="kontakt" type="rtv:typeKontakt" use="required"/>
			<attribute name="annenInstans" type="string" use="optional"/>
		</complexType>
	</element>
	<element name="LegeerklaringGjelder">
		<annotation>
			<documentation>For å angi om legeerklæringen gjelder sykefravær i mer enn 8 uker, rehabiliteringspenger, yrkesrettet attføring eller uførepensjon.  Det benyttes en forekomst av klassen for hver valgmulighet (tilsvarer et kryss på skjemaet). Hvis både felt 0.1 og felt 0.3 er avkrysset må meldingen inneholde to forekomster av klassen.'</documentation>
		</annotation>
		<complexType>
			<attribute name="typeLegeerklaring" type="rtv:typeLegeerklaring"/>
		</complexType>
	</element>
	<element name="Prognose">
		<annotation>
			<documentation>For å angi prognose for bedring av arbeidsevnen, varighet av sykdom/skade, varighet av funksjonsnedsettelse, varighet av nedsatt arbeidsevne.</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="AntattVarighet" type="rtv:typeTekst" minOccurs="0"/>
				<element name="VarighetFunksjonsnedsettelse" type="rtv:typeTekst" minOccurs="0"/>
				<element name="VarighetNedsattArbeidsevne" type="rtv:typeTekst" minOccurs="0"/>
			</sequence>
			<attribute name="bedreArbeidsevne" type="rtv:typeJaNei"/>
		</complexType>
	</element>
	<element name="VurderingArbeidsevne">
		<annotation>
			<documentation>For å angi vurdering av arbeidsevne, begrensninger i forhold til arbeidet pasienten er sykmeldt fra og begrensninger ved vurdering av alternativt arbeid eller yrke.</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="IkkeGjore" type="rtv:typeTekst" minOccurs="0"/>
				<element name="HensynAnnetYrke" type="rtv:typeTekst" minOccurs="0"/>
			</sequence>
			<attribute name="gjenopptaArbeid" type="rtv:typeJaNei" use="optional"/>
